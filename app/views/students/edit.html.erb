<h1>Editting Form For Student</h1>


<%= form_for @student do |c| %>
   <%= c.label "Student First Name: " %><br>
   <%= c.text_field :first_name %><br>
   <%= c.label "Student Last Name: " %><br>
   <%= c.text_field :last_name %><br>
   <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
   <%= c.submit "Update Student"%>
<% end %>